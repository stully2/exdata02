MySql <-"SELECT
cc.CC_ACCT_NUM as [Account_#]
, gl.OWNER_PCT/100 as [Ownership]
, oas.SPID_NUM as [SPID_AS]
, ods.SPID_NUM as [SPID_DS]
, ovp.SPID_NUM as [SPID_VP]
, osv.SPID_NUM as [SPID_SV]
, dt.[ACTUAL_DATE ] as [o_Date]
FROM
V_DIM1_DATE dt
INNER JOIN dbo.FACT_DAYS_STAYED_GOAL_DAILY gl
ON (dt.ACTUAL_DATE=gl.ACTUAL_DATE)
INNER JOIN DIM1_CORP_CLIENT cc
ON (cc.CORP_CLIENT_ID=gl.CORP_CLIENT_ID)
INNER JOIN DIM2_EMPLOYEE  oas
ON (oas.EFFECT_TO_DT IS NULL AND oas.CUR_EMPLOYEE_ID=gl.CUR_AE_ID)
INNER JOIN DIM2_EMPLOYEE  ods
ON (ods.EFFECT_TO_DT IS NULL AND ods.CUR_EMPLOYEE_ID=gl.CUR_RSM_ID)
INNER JOIN DIM2_EMPLOYEE  ovp
ON (ovp.EFFECT_TO_DT IS NULL AND ovp.CUR_EMPLOYEE_ID=gl.CUR_VP_ID)
INNER JOIN DIM2_EMPLOYEE  osv
ON (ovp.EFFECT_TO_DT IS NULL AND osv.CUR_EMPLOYEE_ID=gl.CUR_SVP_ID)
WHERE
dt.ACTUAL_DATE
in('08/31/2015','09/16/2015')
AND
osv.SPID_NUM  Is Not Null
AND
osv.SPID_NUM  <>
'6A4V'
GROUP BY
cc.CC_ACCT_NUM
, gl.OWNER_PCT
, oas.SPID_NUM
, ods.SPID_NUM
, ovp.SPID_NUM
, osv.SPID_NUM
, dt.[ACTUAL_DATE ]
;"
x = sqlQuery(cn,MySql)
head(x)
typeof(x[1,7])
